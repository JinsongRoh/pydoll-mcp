{
  "name": "pydoll-mcp",
  "description": "Revolutionary browser automation MCP server powered by PyDoll - Zero WebDriver automation with AI-powered captcha bypass",
  "version": "1.5.6",
  "author": "JinsongRoh",
  "homepage": "https://github.com/JinsongRoh/pydoll-mcp",
  "repository": "https://github.com/JinsongRoh/pydoll-mcp",
  "license": "MIT",
  "categories": [
    "automation",
    "browser",
    "web-scraping",
    "testing",
    "ai"
  ],
  "features": [
    "Zero WebDriver browser automation",
    "AI-powered Cloudflare & reCAPTCHA bypass",
    "Human behavior simulation",
    "79 powerful automation tools",
    "Chrome DevTools Protocol integration",
    "Natural attribute element finding",
    "Advanced stealth mode",
    "Network interception and monitoring",
    "Screenshot and PDF generation",
    "Cross-platform support"
  ],
  "requirements": {
    "python": ">=3.8",
    "mcp": ">=1.0.0"
  },
  "installation": {
    "type": "pip",
    "package": "pydoll-mcp",
    "command": "pip install pydoll-mcp"
  },
  "configuration": {
    "mcpServers": {
      "pydoll": {
        "command": "python",
        "args": ["-m", "pydoll_mcp.server"],
        "env": {
          "PYTHONIOENCODING": "utf-8",
          "PYDOLL_LOG_LEVEL": "INFO"
        }
      }
    }
  },
  "tags": [
    "pydoll",
    "browser-automation",
    "web-scraping",
    "captcha-bypass",
    "cloudflare",
    "recaptcha",
    "chrome-devtools",
    "mcp-server",
    "ai-automation",
    "stealth-browser"
  ],
  "documentation": {
    "readme": "https://github.com/JinsongRoh/pydoll-mcp/blob/main/README.md",
    "installation": "https://github.com/JinsongRoh/pydoll-mcp/blob/main/INSTALLATION_GUIDE.md",
    "changelog": "https://github.com/JinsongRoh/pydoll-mcp/blob/main/CHANGELOG.md"
  },
  "screenshots": [
    {
      "url": "https://github.com/user-attachments/assets/219f2dbc-37ed-4aea-a289-ba39cdbb335d",
      "description": "PyDoll MCP Server Logo"
